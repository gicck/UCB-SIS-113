# Archivo de configuración de entorno de ejemplo
# Copiar a .env y personalizar para tu entorno

# Rutas de archivos (multiplataforma)
INPUT_DIR=./data
OUTPUT_DIR=./output

# Configuración de base de datos
DB_HOST=localhost
DB_PORT=5432
DB_USER=myapp
DB_PASSWORD=secret

# Configuración de API
API_URL=http://localhost:8000/api
API_TIMEOUT=30

# Entorno
ASPNETCORE_ENVIRONMENT=Development
